{
  "name": "iota-did",
  "version": "1.0.0",
  "description": "A monorepo of IOTA DID senarios",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "examples/"
  ],
  "scripts": {
    "org": "yarn workspace @did/org",
    "wallet": "yarn workspace @did/wallet",
    "test": "yarn workspace @did/test",
    "examples": "yarn workspace @did/examples",
    "iota": "yarn workspace @did/iota",
    "lowdb": "yarn workspace @did/lowdb",
    "org-db": "yarn workspace @did/org-db",
    "wallet-db": "yarn workspace @did/wallet-db",
    "lint": "eslint ./apps/* ./packages/*",
    "format": "prettier --write ./apps/*/src ./packages/*/src",
    "type-check": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tsx": "^4.10.4",
    "typescript": "^5.4.5"
  },
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=20.12.2",
    "yarn": ">=1.22.22"
  }
}
